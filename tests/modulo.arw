                                                
& divide [# left, # right ] X #                   
<
                                                ~Function Declaration, return int~
                                                ~Int assigment~
    # quotient <- 0!
                                                ~While Loop [left > right]~                              
    O left <>> right                              
    <
                                                ~Minus operator reassignment~
        left <- left - right!
                                                ~Plus operator reassignment~                     
        quotient <- quotient + 1!                
    >
                                                ~Return Value~
    quotient -> !                                 
>
                                                
& modulo [# left, # right ] X #                   
<
                                                ~Function Declaration, return int~
                                                ~Multiply operator with function call~ 
    # decrement <- right * divide[left, right]!   
                                                ~Minus operator reassignment~              
    decrement <- left - decrement!                
                                                ~Return Value~
    decrement -> !                                
>

